[user]
	name = Aku Kotkavuo
	email = aku.kotkavuo@gmail.com
[core]
	editor = vim
	excludesfile = ~/.gitignore
[color]
	ui = true
[branch]
	autosetuprebase = always
[push]
	default = simple
[credential]
	helper = osxkeychain
[sendemail]
	smtpencryption = tls
	smtpserver = smtp.gmail.com
	smtpserverport = 587
	smtpuser = aku.kotkavuo@gmail.com
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[gpg]
	program = gpg2
